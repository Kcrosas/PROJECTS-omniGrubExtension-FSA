{
  "name": "OmniGrub",
  "version": "0.1",
  "description": "Takes all displayed search results and does an API call to multiple platforms to add reviews",
  "manifest_version": 2,
  "icons": {
    "128": "icon.png"
  },
  "content_scripts": [
    {
      "css": ["style.css"],
      "js": ["action.js"],
      "matches": ["https://*.doordash.com/store*"]
    }
  ],
  "permissions": ["<all_urls>"],
  "background": {
    "page": "background.html",
    "persistent": false
  },
  "web_accessible_resources": [
    "small_1_half@3x.png",
    "small_2.5.png",
    "yelp-logo.png",
    "small_0.png",
    "small_1.png",
    "small_1.5.png",
    "small_2.png",
    "small_3.png",
    "small_3.5.png",
    "small_4.png",
    "small_4.5.png",
    "small_5.png",
    "external.png"
  ]
}
